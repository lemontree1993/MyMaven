                        <div class="col-sm-5">
                            <input type="password" class="form-control" id="re_password" placeholder="密码确认">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="reg_phone" class="col-sm-4 control-label">手机号码:</label>
                        <div class="col-sm-5">
                            <input type="text" class="form-control" id="reg_phone" placeholder="手机号码">
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="reg_email" class="col-sm-4 control-label">邮箱:</label>
                        <div class="col-sm-5">
                            <input type="text" class="form-control" id="reg_email" placeholder="邮箱">
                        </div>
                    </div>
